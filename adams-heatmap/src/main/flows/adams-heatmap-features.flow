adams.flow.control.Flow
	-annotation
	Shows how to generate features from heatmaps.
	-actor
		adams.flow.source.FileSupplier
			-file
			${EXAMPLE_FLOWS}/data/3666455665_18795f0741.jpg
			-file
			${EXAMPLE_FLOWS}/data/4825046958_787c97bc96.jpg
	-actor
		adams.flow.transformer.HeatmapFileReader
			-reader
				adams.data.io.input.SimpleImageHeatmapReader
					-reader
						adams.data.io.input.JAIImageReader
	-actor
		adams.flow.transformer.HeatmapFeatureGenerator
			-algorithm
				adams.data.heatmapfeatures.MultiFeatureGenerator
					-converter
						adams.data.featureconverter.SpreadSheet
							-data-row-type
								adams.data.spreadsheet.DenseDataRow
							-spreadsheet-type
								adams.data.spreadsheet.SpreadSheet
					-sub-generator
						adams.data.heatmapfeatures.Min
							-converter
								adams.data.featureconverter.SpreadSheet
									-data-row-type
										adams.data.spreadsheet.DenseDataRow
									-spreadsheet-type
										adams.data.spreadsheet.SpreadSheet
					-sub-generator
						adams.data.heatmapfeatures.Max
							-converter
								adams.data.featureconverter.SpreadSheet
									-data-row-type
										adams.data.spreadsheet.DenseDataRow
									-spreadsheet-type
										adams.data.spreadsheet.SpreadSheet
					-sub-generator
						adams.data.heatmapfeatures.Mean
							-converter
								adams.data.featureconverter.SpreadSheet
									-data-row-type
										adams.data.spreadsheet.DenseDataRow
									-spreadsheet-type
										adams.data.spreadsheet.SpreadSheet
					-sub-generator
						adams.data.heatmapfeatures.Median
							-converter
								adams.data.featureconverter.SpreadSheet
									-data-row-type
										adams.data.spreadsheet.DenseDataRow
									-spreadsheet-type
										adams.data.spreadsheet.SpreadSheet
					-sub-generator
						adams.data.heatmapfeatures.StandardDeviation
							-converter
								adams.data.featureconverter.SpreadSheet
									-data-row-type
										adams.data.spreadsheet.DenseDataRow
									-spreadsheet-type
										adams.data.spreadsheet.SpreadSheet
					-sub-generator
						adams.data.heatmapfeatures.Histogram
							-converter
								adams.data.featureconverter.SpreadSheet
									-data-row-type
										adams.data.spreadsheet.DenseDataRow
									-spreadsheet-type
										adams.data.spreadsheet.SpreadSheet
	-actor
		adams.flow.transformer.SpreadSheetRowBuffer
	-actor
		adams.flow.sink.SpreadSheetDisplay
			-writer
				adams.data.io.output.NullWriter
	-flow-execution-listener
		adams.flow.execution.NullListener

